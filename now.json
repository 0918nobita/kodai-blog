{
  "version": 2,
  "builds": [
    {
      "src": "workspaces/static/package.json",
      "use": "@now/static-build"
    },
    {
      "src": "workspaces/static/dist/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Service-Worker-Allowed": "/"
      },
      "dest": "/workspaces/static/dist/$1"
    }
  ]
}
