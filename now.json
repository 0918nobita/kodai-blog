{
  "version": 2,
  "builds": [
    {
      "src": "workspaces/static/build.sh",
      "use": "@now/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "workspaces/static/dist/**/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "Service-Worker-Allowed": "/"
      },
      "dest": "/workspaces/static/dist/$1"
    }
  ]
}
