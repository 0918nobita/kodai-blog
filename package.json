    "react": "16.13.1",
    "react-dom": "16.13.1",
    "tailwindcss": "^1.4.6"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/node": "13.13.10",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "@typescript-eslint/parser": "3.1.0",
    "autoprefixer": "9.8.0",
    "contentful": "7.14.4",
    "cssnano": "4.1.10",
