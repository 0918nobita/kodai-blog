{
    "name": "kodai-vision",
    "version": "1.0.0",
    "license": "MIT",
    "private": true,
    "scripts": {
        "gentypes": "contentful-typescript-codegen --output contentful.d.ts",
        "dev": "next",
        "build": "next build && next export",
        "lint": "run-p lint:ts-js lint:css",
        "lint:ts-js": "eslint './**/*.{ts,tsx,js}'",
        "lint:css": "stylelint './**/*.css'",
        "format": "run-p format:ts-js format:css",
        "format:ts-js": "eslint --fix './**/*.{ts,tsx,js}'",
        "format:css": "stylelint --fix './**/*.css'",
        "test": "jest",
        "test:update": "jest --updateSnapshot"
    },
    "dependencies": {
        "@contentful/rich-text-react-renderer": "14.1.1",
        "contentful-management": "6.0.3",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "tailwindcss": "1.5.2",
        "three": "0.118.3"
    },
    "devDependencies": {
        "@types/jest": "26.0.8",
        "@types/node": "13.13.10",
        "@types/react": "16.9.43",
        "@types/react-dom": "16.9.8",
        "@types/react-test-renderer": "16.9.2",
        "@typescript-eslint/eslint-plugin": "3.7.0",
        "@typescript-eslint/parser": "3.7.0",
        "autoprefixer": "9.8.6",
        "contentful": "7.14.6",
        "contentful-typescript-codegen": "3.2.1",
        "cssnano": "4.1.10",
        "dotenv": "8.2.0",
        "eslint": "7.5.0",
        "eslint-config-prettier": "6.11.0",
        "eslint-plugin-prettier": "3.1.4",
        "eslint-plugin-react": "7.20.5",
        "eslint-plugin-simple-import-sort": "5.0.3",
        "jest": "26.1.0",
        "jest-css-modules": "2.1.0",
        "next": "9.4.4",
        "next-compose-plugins": "2.2.0",
        "next-optimized-images": "2.6.2",
        "next-pwa": "3.1.1",
        "npm-run-all": "4.1.5",
        "postcss-flexbugs-fixes": "4.2.1",
        "postcss-preset-env": "6.7.0",
        "prettier": "2.0.5",
        "react-test-renderer": "16.13.1",
        "stylelint": "13.6.1",
        "stylelint-config-standard": "20.0.0",
        "ts-jest": "26.1.4",
        "typescript": "3.9.7",
        "webpack": "4.44.1"
    },
    "engines": {
        "node": "12.x"
    },
    "browserslist": {
        "browsers": "defaults"
    }
