    "format:ts": "eslint --fix './**/*.{ts,tsx}'",
    "format:js": "prettier --write './**/*.js'",
    "format:css": "stylelint --fix './**/*.css'",
    "test": "jest",
    "test:update": "jest --updateSnapshot"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-rc.2",
