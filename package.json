    "jest-css-modules": "^2.1.0",
    "next": "9.4.4",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.6.1",
    "next-pwa": "2.5.0",
    "npm-run-all": "4.1.5",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.0.5",
    "react-test-renderer": "^16.13.1",
    "stylelint": "13.6.0",
    "stylelint-config-standard": "20.0.0",
    "ts-jest": "^26.1.0",
    "typescript": "3.9.5",
    "webpack": "4.43.0"
